{
    "rewrites": [
        {
            "source": "/supabase/:match*",
            "destination": "https://vewqzsqddgmkslnuctvb.supabase.co/:match*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "no-cache, no-store, must-revalidate"
                }
            ]
        }
    ]
}